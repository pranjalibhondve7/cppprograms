void sum(int a,int b )
{ 
//int a= 10,b= 20;
int c= a+b;
cout<<"Addition is :"<<c<<endl;
}
void sub(int a,int b)
{ 
//int a= 130,b= 120;
int c= a-b;
cout<<"substraction  is :"<<c<<endl;
}
void mul(int a,int b )
{ 
//int a= 10,b= 20;
int c= a*b;
cout<<" Multiplication is :"<<c<<endl;
}
