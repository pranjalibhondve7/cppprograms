void arect(float l,float b)
{ 
//int l= 10,b= 20;
int A= l*b;
cout<<"Area of rectangle is :"<<A<<endl;
}
void asqr(float a)
{ 
//int a= 10;
int A=a*a;
cout<<"Area of square is :"<<A<<endl;
}
void acircle(float r)
{ 
//int r=8;
float A=3.14*r*r;
cout<<"Area of circle  is :"<<A<<endl;
}
void atriangle(float b,float h)
{ 
//int b= 10,h= 20;
float A= 0.5*b*h;
cout<<"Area of triange is :"<<A<<endl;
}
void aparallelogram(float b,float h)
{ 
//int b= 10,h= 20;
int A= b*h;
cout<<"Area of parallelogram is :"<<A<<endl;
}
void aellipse(float r1,float r2)
{ 
//int r1= 10,r2= 20;
float A= 3.14*r1*r2;
cout<<"Area of ellipse is :"<<A<<endl;
}
void vcube(float a)
{ 
//int a= 10;
int V= a*a*a;;
cout<<"Volunme of cube  is :"<<V<<endl;
}
void asphere(float r)
{ 
//int r=10;
float A= 4*3.14*r*r;
cout<<"Area of sphere  is :"<<A<<endl;
}
void psqr(int a)
{ 
//int a= 10;
int P= 4*a;
cout<<"Perimeter of square  is :"<<P<<endl;
}
void ccircle(int r)
{ 
//int r=8;
float C= 2*3.14*r;
cout<<"Circumference of circle is :"<<C<<endl;
}
